datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model CmdbData {
  createdAt   DateTime  @default(now())
  id          String    @id @default(cuid())
  itemDetails String?
  itemName    String?
  lastUpdated DateTime?
  updatedAt   DateTime  @updatedAt
}

model ArisData {
  attributeName  String?
  attributeValue String?
  createdAt      DateTime  @default(now())
  id             String    @id @default(cuid())
  lastSynced     DateTime?
  updatedAt      DateTime  @updatedAt
}

model TokenStorage {
  createdAt   DateTime  @default(now())
  expiry      DateTime?
  id          String    @id @default(cuid())
  serviceName String?
  token       String?
  updatedAt   DateTime  @updatedAt
}
